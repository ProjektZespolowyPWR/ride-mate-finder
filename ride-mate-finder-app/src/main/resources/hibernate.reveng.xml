<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
    <schema-selection match-schema="public"/>
    <table-filter match-name="flyway_schema_history" exclude="true"/>
    <type-mapping>
        <sql-type jdbc-type="OTHER" hibernate-type="uuid-binary"/>
    </type-mapping>
</hibernate-reverse-engineering>